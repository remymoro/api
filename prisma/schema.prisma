

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Center {
  id         Int      @id @default(autoincrement())
  name       String
  city       String
  codePostal String
  address    String
  phone      String?
  email      String?

  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
