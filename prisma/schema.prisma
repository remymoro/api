

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Center {
  id         Int      @id @default(autoincrement())
  name       String
  city       String
  codePostal String    // <-- CHANGER Int â†’ String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
